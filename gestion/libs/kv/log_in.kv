<MyMDLabel@MDLabel>
    


AnchorLayout:
    anchor_x: "center"
    anchor_y: "center"
    BoxLayout:
        orientation: "vertical"
        size_hint: .9,.9
        MyMDLabel:
            font_style: 'Overline'
            theme_text_color: 'Primary'
            text: "Overline label"*100
            halign: 'center'

        MyMDLabel:
            font_style: 'Caption'
            theme_text_color: 'Primary'
            text: "Caption label"
            halign: 'center'

        MyMDLabel:
            font_style: 'Button'
            theme_text_color: 'Primary'
            text: "Button label"
            halign: 'center'
